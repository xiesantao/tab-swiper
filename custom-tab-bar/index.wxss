.text {
  color: #A1A6AB;
  font-size: 18rpx;
  font-weight: 400;
  font-family: PingFangSC-Regular, PingFang SC;
  line-height: 26rpx;
  text-align: center;
  margin-top: -8rpx;
}

.active {
  color: #0A6885;
}

.image {
  width: 60rpx;
  height: 60rpx;
}

.bottom-bar {
  display: flex;
  justify-content: space-between;
  padding: 24rpx 86rpx 58rpx;
  background-color: #fff;
  height: 166rpx;
  box-shadow: 0 -10rpx 20rpx 0 rgba(0, 0, 0, 0.03);
  box-sizing: border-box;
  position:fixed;
  left:0;
  right:0;
  bottom:0;
  z-index:10;
}